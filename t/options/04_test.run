#!/bin/sh
set -e

[ -f out.xml ] && rm out.xml
cp orig.xml test1.xml
cp orig.xml test2.xml
$XMLPATCH -s -o out.xml < 04_patch.xml

diff out.xml 04_result.xml > /dev/null || exit 1
diff orig.xml test1.xml > /dev/null && exit 1
diff orig.xml test2.xml > /dev/null && exit 1
[ -f test1.xml.rej ] && exit 1
[ -f test2.xml.rej ] && exit 1
